- if controller_name != 'sessions'
  .login_switching_btn
    = link_to new_session_path(resource_name), class: 'login_switching_btn_text' do
      ログインへ
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  .login_switching_btn
    = link_to new_registration_path(resource_name), class: 'login_switching_btn_text' do
      新規登録へ
  %br/
